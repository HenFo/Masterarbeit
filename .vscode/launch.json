    "version": "0.2.0",
    "configurations": [
        {
            "name": "Accelerate Debugger: Main.py",
            "type": "debugpy",
            "request": "launch",
            "program": ".conda/bin/accelerate",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
